{
	"name": "vscode-codeception",
	"displayName": "Codeception test API",
	"description": "VSCode extension to use codeception with test API ",
	"version": "0.0.1",
	"publisher": "AlexisPPLIN",
	"private": true,
	"license": "MIT",
	"repository": "https://github.com/AlexisPPLIN/vscode-codeception",
	"engines": {
		"vscode": "^1.88.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:*.md"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^18",
		"@types/vscode": "^1.88.0",
		"@typescript-eslint/eslint-plugin": "^7.14.0",
		"@typescript-eslint/parser": "^7.14.0",
		"eslint": "^8.26.0",
		"typescript": "^5.5.2"
	}
}
